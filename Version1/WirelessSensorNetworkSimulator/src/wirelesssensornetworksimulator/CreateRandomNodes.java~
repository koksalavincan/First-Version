public class CreateRandomNodes {
    private int numNodes;
    public void getNumNodes(){
        System.out.println("Enter the number of nodes:   " );
        numNodes = 10;
    }
    public void showNodes() {
        System.out.println("X = " + numNodes);
    }
}
